{
  "name": "@taskforcesh/bullmq-pro",
  "version": "7.26.1",
  "description": "BullMQ Professional",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "jsnext:main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "source": "src/index.ts",
  "author": "Taskforce.sh Inc.",
  "license": "Commercial",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.taskforce.sh"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm-run-all pretest tsc:all copy:lua copy:includes:lua clean:temp:files",
    "circular:references": "madge --circular --extensions ts ./src",
    "clean:scripts": "rimraf rawScripts src/scripts",
    "clean:temp:files": "rimraf dist/cjs/bullmq.d.ts dist/esm/bullmq.d.ts dist/tsdoc-metadata.json",
    "cm": "git cz",
    "copy:lua": "copyfiles -f ./src/commands/*.lua ./dist/cjs/commands && copyfiles -f ./src/commands/*.lua ./dist/esm/commands",
    "copy:includes:lua": "copyfiles -f ./src/commands/includes/*.lua ./dist/cjs/commands/includes && copyfiles -f ./src/commands/includes/*.lua ./dist/esm/commands/includes",
    "dc:up": "docker-compose -f docker-compose.yml up -d",
    "dc:down": "docker-compose -f docker-compose.yml down",
    "docs": "typedoc --options ./typedoc.json --excludeExternals --excludeProtected --excludePrivate --readme none src/index.ts",
    "eslint:fix": "./node_modules/.bin/eslint . --ignore-path ./.eslintignore --fix",
    "lint": "./node_modules/.bin/eslint . --ignore-path ./.eslintignore",
    "lint:staged": "./node_modules/.bin/lint-staged",
    "generate:raw:scripts": "ts-node --project tsconfig-cjs.json scripts/generateRawScripts.ts",
    "precommit": "npm-run-all pretty:quick lint:staged",
    "pretest": "npm-run-all clean:scripts generate:raw:scripts transform:commands",
    "prettier": "prettier --config package.json src/**/*.ts",
    "pretty:quick": "./node_modules/.bin/pretty-quick --ignore-path ./.eslintignore --staged",
    "test": "ts-mocha -p tsconfig-cjs.json --config ./.mocharc.js",
    "test:stress": "ts-mocha -p tsconfig-cjs.json --config ./.mocharc-stress.js",
    "pretest:bullmq": "npm run pretest",
    "test:bullmq": "ts-mocha -p tsconfig-cjs.json --config ./.mocharc-bullmq.js",
    "test:watch": "ts-mocha --paths 'src/**/test_*.ts' -w --watch-extensions ts",
    "transform:commands": "node ./scripts//commandTransform.js ./rawScripts ./src/scripts",
    "tsc": "tsc",
    "tsc:all": "tsc && tsc -p tsconfig-cjs.json"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^9.0.1",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.9",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@types/chai": "^4.2.16",
    "@types/chai-as-promised": "^7.1.3",
    "@types/lodash": "^4.14.172",
    "@types/mocha": "^5.2.5",
    "@types/node": "^12.6.8",
    "@types/semver": "^6.0.1",
    "@types/sinon": "^7.0.13",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.2.4",
    "copyfiles": "^2.1.1",
    "cron-parser": "^4.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-tsdoc": "^0.2.14",
    "fast-glob": "^3.3.2",
    "husky": "^3.0.3",
    "ioredis": "^5.3.2",
    "istanbul": "^0.4.5",
    "lint-staged": "10.5.4",
    "lodash": "^4.17.21",
    "madge": "^5.0.1",
    "minimatch": "^9.0.3",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "moment-timezone": "^0.5.33",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "progress": "^2.0.3",
    "rimraf": "^3.0.2",
    "rrule": "^2.6.9",
    "semantic-release": "^17.4.7",
    "sinon": "^7.2.2",
    "test-console": "^2.0.0",
    "ts-mocha": "^8.0.0",
    "ts-node": "^8.10.2",
    "typedoc": "^0.24.0",
    "typescript": "^4.7.4",
    "xstate": "^4.26.1"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts}": "yarn eslint:fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/gitbook/changelog.md"
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "node scripts/updateVersion.js ${nextRelease.version} && yarn build"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "yarn.lock",
            "src/version.ts",
            "docs/gitbook/changelog.md",
            "docs/gitbook/api/**"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "dependencies": {
    "bullmq": "5.34.4",
    "msgpackr": "^1.11.2",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "uuid": "^9.0.0"
  }
}
